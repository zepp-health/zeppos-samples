{
  "configVersion": "v3",
  "app": {
    "appId": 1095465,
    "appName": "T9 Keyboard",
    "appType": "app",
    "version": {
      "code": 110,
      "name": "1.1.0"
    },
    "icon": "icon.png",
    "vender": "zepp",
    "description": "T9 Predictive Text Keyboard for ZeppOS"
  },
  "permissions": ["device:os.input.method", "data:os.device.info"],
  "runtime": {
    "apiVersion": {
      "compatible": "4.0",
      "target": "4.0",
      "minVersion": "4.2"
    }
  },
  "targets": {
    "default": {
      "module": {
        "page": {
          "pages": ["pages/guidelines", "data-widget/index"]
        },
        "data-widget": {
          "widgets": [
            {
              "name": "English",
              "path": "data-widget/index",
              "icon": "icon.png",
              "runtime": {
                "ability": [
                  {
                    "name": "en-US",
                    "type": 2,
                    "subType": []
                  }
                ]
              }
            }
          ]
        }
      },
      "platforms": [
        {
          "st": "r",
          "dw": 480
        },
        {
          "st": "s",
          "dw": 390
        }
      ]
    }
  },
  "i18n": {
    "en-US": {
      "appName": "T9 Keyboard"
    },
    "zh-CN": {
      "appName": "T9 键盘"
    }
  },
  "defaultLanguage": "en-US"
}